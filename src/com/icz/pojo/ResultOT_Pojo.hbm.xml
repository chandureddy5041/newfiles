<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.icz.pojo.ResultOT_Pojo" table="result_ot">

		<id name="r_id">
			<generator class="increment"></generator>
		</id>
		<property name="u_id"></property>
		<property name="fname"></property>
		<property name="lname"></property>
		<property name="dob"></property>
		<property name="gender"></property>
		<property name="email"></property>
		<property name="mobile"></property>
		<property name="total_questions"></property>
		<property name="correct_answers"></property>
		

	</class>

</hibernate-mapping>